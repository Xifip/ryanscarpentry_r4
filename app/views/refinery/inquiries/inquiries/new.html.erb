<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header"><%= @page.title %>
                    </h1>
         <ol class="breadcrumb">
            <li><a href="/"><%= Refinery::Page.find_by(link_url: "/").title %></a>
            </li>
            <li class="active"><%= @page.title %></li>
        </ol>
    </div>
</div>
<div class="row">
  <div id="googleMap" class="col-md-8 col-sm-12"></div>
  <div class="col-md-4">
     <%= raw(@page.content_for(:side_body)) %>
  </div>
</div>
<div class="row">
  <div class="inquiries col-md-8">
    <h3><%= raw(@page.content_for(:body)) %></h3>
    <%= render "/refinery/inquiries/inquiries/form", inquiry: @inquiry %>
  </div>
</div>
